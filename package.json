{
  "name": "server",
  "version": "1.0.0",
  "description": "Example from Node with React: Fullstack Web Development",
  "main": "index.js",
  "engines": {
    "node": "8.10.0",
    "npm": "6.12.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
